{
  "inputFileTypes": ["in", "data"],
  "outputFileTypes": ["ans", "out", "sol", "a"],
  "defaultTimeLimit": "5000 ms",
  "minimumTimeLimit": "100 ms",
  "maximumTimeLimit": "3600000 ms",
  "languageSpecs": [
    {
      "languageName": "Java",
      "detectLanguagePriority": 1,
      "isAllowed": true,
      "fileExtensions": ["java"],
      "needsCompile": true,
      "compileArgs": ["javac", "-cp", ".;*", "{file}"],
      "runtimeArgs": ["java", "-Xmx1024M", "-Xss512M", "-DONLINE_JUDGE=true", "-Duser.language=en", "-Duser.region=US", "-Duser.variant=US", "{filename}"]
    },
    {
      "languageName": "GNU GCC C11 5.1.0",
      "detectLanguagePriority": 1,
      "isAllowed": true,
      "fileExtensions": ["c"],
      "needsCompile": true,
      "compileArgs": ["gcc", "-lm", "-O2", "-std=c11", "-o", "{filename}.exe", "{file}"],
      "runtimeArgs": ["{dir}/{filename}.exe"]
    },
    {
      "languageName": "GNU G++11 5.1.0",
      "detectLanguagePriority": 1,
      "isAllowed": true,
      "fileExtensions": ["cpp", "cc"],
      "needsCompile": true,
      "compileArgs": ["g++", "-lm", "-O2", "-std=c++11", "-o", "{filename}.exe", "{file}"],
      "runtimeArgs": ["{dir}/{filename}.exe"]
    },
    {
      "languageName": "GNU G++14 6.4.0",
      "detectLanguagePriority": 2,
      "isAllowed": true,
      "fileExtensions": ["cpp", "cc"],
      "needsCompile": true,
      "compileArgs": ["g++", "-lm", "-O2", "-std=c++14", "-o", "{filename}.exe", "{file}"],
      "runtimeArgs": ["{dir}/{filename}.exe"]
    },
    {
      "languageName": "GNU G++17 7.2.0",
      "detectLanguagePriority": 3,
      "isAllowed": true,
      "fileExtensions": ["cpp", "cc"],
      "needsCompile": true,
      "compileArgs": ["g++", "-lm", "-O2", "-std=c++17", "-o", "{filename}.exe", "{file}"],
      "runtimeArgs": ["{dir}/{filename}.exe"]
    },
    {
      "languageName": "Python 3.6",
      "detectLanguagePriority": 1,
      "isAllowed": true,
      "fileExtensions": ["py"],
      "needsCompile": false,
      "runtimeArgs": ["python3", "{file}"]
    },
    {
      "languageName": "Python 2.7",
      "detectLanguagePriority": 2,
      "isAllowed": true,
      "fileExtensions": ["py"],
      "needsCompile": false,
      "runtimeArgs": ["python2", "{file}"]
    },
    {
      "languageName": "Pascal",
      "detectLanguagePriority": 1,
      "isAllowed": true,
      "fileExtensions": ["pas"],
      "needsCompile": true,
      "compileArgs": ["fpc", "-O3", "-o{filename}.exe", "{file}"],
      "runtimeArgs": ["{dir}/{filename}.exe"]
    },
    {
      "languageName": "Pre-Compiled Executable",
      "detectLanguagePriority": 2,
      "isAllowed": true,
      "fileExtensions": ["exe"],
      "needsCompile": false,
      "runtimeArgs": ["./{file}"]
    }
  ]
}
